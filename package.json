{
  "name": "vite-electron-builder",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "pretest": "npm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer",
    "pretypecheck-renderer": "dts-cb -i packages/preload/src/**/*.ts -o packages/preload/exposedInMainWorld.d.ts"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "2.2.1",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "cross-env": "7.0.3",
    "dts-for-context-bridge": "0.7.1",
    "electron": "16.0.5",
    "electron-builder": "22.14.5",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "nano-staged": "0.5.0",
    "playwright": "1.17.1",
    "prettier": "2.5.1",
    "simple-git-hooks": "2.7.0",
    "typescript": "4.5.4",
    "vite": "2.7.10"
  },
  "dependencies": {
    "@codemirror/commands": "0.19.6",
    "@codemirror/gutter": "0.19.9",
    "@codemirror/highlight": "0.19.7",
    "@codemirror/history": "0.19.2",
    "@codemirror/lang-javascript": "0.19.4",
    "@codemirror/lang-markdown": "0.19.4",
    "@codemirror/language": "0.19.7",
    "@codemirror/language-data": "0.19.1",
    "@codemirror/matchbrackets": "0.19.3",
    "@codemirror/state": "0.19.6",
    "@codemirror/theme-one-dark": "0.19.1",
    "assert": "2.0.0",
    "electron-updater": "4.6.5",
    "github-markdown-css": "5.1.0",
    "process": "0.11.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark-gfm": "3.0.1",
    "remark-parse": "10.0.1",
    "remark-react": "9.0.1",
    "unified": "10.1.1"
  }
}
